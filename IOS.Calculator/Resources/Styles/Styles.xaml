<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Style TargetType="Grid" >
        <Setter Property="BackgroundColor" Value="{StaticResource Black}"></Setter>
    </Style>

    <Style TargetType="Button">
        <Setter Property="TextColor" Value="White"></Setter>
        <Setter Property="FontFamily" Value="SFPRO-Regular"></Setter>
        <Setter Property="FontSize" Value="35"></Setter>
        <Setter Property="CornerRadius" Value="50"></Setter>
        <Setter Property="HeightRequest" Value="84"></Setter>
        <Setter Property="WidthRequest" Value="82"></Setter>
        <Setter Property="Margin" Value="7"></Setter>
        <Setter Property="BackgroundColor" Value="{StaticResource Tertiary}"></Setter>
    </Style>

</ResourceDictionary>
